{
    "name": "mvdnbrk/gtin",
    "description": "GTIN / EAN / UPC Validation for Laravel",
    "keywords": [
        "laravel",
        "validation",
        "validator",
        "ean",
        "upc",
        "gtin",
        "gs1"
    ],
    "homepage": "https://github.com/mvdnbrk/gtin",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark van den Broek",
            "email": "mvdnbrk@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/validation": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.9|^3.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "pestphp/pest": "^1.23.0|^2.34.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mvdnbrk\\Gtin\\GtinServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Mvdnbrk\\Gtin\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mvdnbrk\\Gtin\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
